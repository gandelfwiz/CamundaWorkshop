    <bpmn:userTask id="SelectAuthorizationType" name="Select the authorization type">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="authorizationType" label="Authorization Type" type="enum">
            <camunda:value id="PASSWORD" name="Password" />
            <camunda:value id="BIOMETRIC" name="Biometric" />
          </camunda:formField>
        </camunda:formData>
