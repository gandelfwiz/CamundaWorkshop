            <camunda:inputOutput>
              <camunda:inputParameter name="headers">
                <camunda:map>
                  <camunda:entry key="Content-Type">application/json</camunda:entry>
                  <camunda:entry key="Accept">application/json</camunda:entry>
                </camunda:map>
              </camunda:inputParameter>
              <camunda:inputParameter name="method">POST</camunda:inputParameter>
              <camunda:inputParameter name="url">http://localhost:8900/external-service/sms</camunda:inputParameter>
              <camunda:inputParameter name="payload">
                <camunda:script scriptFormat="javascript">var payload = {
    "phoneNumber": "+39111223344",
    "message": "Send sms from Camunda Process for reason code " + execution.getVariable("escalateCode")
}

JSON.stringify(payload);</camunda:script>
              </camunda:inputParameter>
              <camunda:outputParameter name="response">${S(response)}</camunda:outputParameter>
            </camunda:inputOutput>
            <camunda:connectorId>http-connector</camunda:connectorId>
          </camunda:connector>
          <camunda:inputOutput>
            <camunda:outputParameter name="resultSms">${response}</camunda:outputParameter>
          </camunda:inputOutput>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_006xm1j</bpmn:incoming>
        <bpmn:outgoing>Flow_0ekltsu</bpmn:outgoing>
