    <bpmn:sequenceFlow id="Flow_02hv2i9" sourceRef="SelectAuthorizationType" targetRef="Gateway_0px812s" />
    <bpmn:exclusiveGateway id="Gateway_0px812s">
      <bpmn:incoming>Flow_02hv2i9</bpmn:incoming>
      <bpmn:outgoing>biometricFlow</bpmn:outgoing>
      <bpmn:outgoing>passwordFlow</bpmn:outgoing>
