    <bpmn:sequenceFlow id="Flow_0bw2326" sourceRef="subProcessOtherDevice" targetRef="Gateway_0mx8n6w" />
    <bpmn:sequenceFlow id="biometricFlow" name="biometric" sourceRef="Gateway_0px812s" targetRef="subProcessOtherDevice">
