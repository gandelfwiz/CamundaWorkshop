      "name": "workflowId",
      "type": "string"
    },
    {
      "name": "taskId",
      "type": "string"
    },
    {
      "name": "feedbackRequired",
      "type": "boolean"
    },
    {
      "name": "feedback",
      "type": [
        "null",
        {
          "name": "FeedbackRecord",
          "type": "record",
          "fields": [
            {
              "name": "feedbackEvent",
              "type": "string"
            },
            {
              "name": "feedbackType",
              "type": {
                "type": "enum",
                "symbols": [
                  "SIGNAL",
                  "MESSAGE"
                ],
                "name": "FeedbackTypeEnum"
              }
            }
          ]
        }
      ],
      "default": null
    },
    {
      "name": "data",
      "type": {
        "type": "map",
        "values": "string"
      }
    }
  ]
}